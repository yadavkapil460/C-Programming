//absolute value of an integer 
#include<stdio.h>
int main(){
    int n;
    printf("Enter n : ");
    scanf("%d",&n);
    if( n < 0){
        n = n*(-1);
        printf("The absolute value of integer is : %d",n);
    }
    else{
        printf("The absolute value of integer is : %d",n);
    }
     
    return 0;
}
