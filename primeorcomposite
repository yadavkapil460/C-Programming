//to check whether prime or composite 
#include<stdio.h>
int main(){
int n;
printf("Enter n : ");
scanf("%d",&n);
int a = 0;
for(int i=2 ; i<=n-1 ; i++){
if(n%i==0){
(a=1);
printf("neither prime nor composite");
break;
}
else if( a==0){
printf("prime no.");
}
else{
printf("composite no.");
}
}
return 0;
}
